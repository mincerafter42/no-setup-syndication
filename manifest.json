{
	"manifest_version": 2,
	"version": "0",
	"name": "No Setup Syndication",
	"short_name": "NSS",
	"permissions": [
		"storage",
		"alarms"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"browser_style": true,
		"chrome_style": true
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false,
		"browser_style": true,
		"chrome_style": true
	},
	"background": {
		"scripts": ["background.js"]
	}
}
